<script>
    function setActive() {
        // Get the current URL path
        const currentPath = window.location.pathname;

        // Loop through all nav links
        document.querySelectorAll('.nav-link').forEach(link => {
            // Remove active class from all nav links
            link.classList.remove('active');

            // Create an anchor element to parse the href attribute
            const linkPath = new URL(link.getAttribute('href')).pathname;

            // Check if the parsed pathname matches the current path
            if (linkPath === currentPath) {
                // Add active class to the matching nav link
                link.classList.add('active');
            }
        });
    }

    // Call setActive when the page loads
    window.onload = setActive;

</script>