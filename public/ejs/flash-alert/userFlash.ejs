      <% if (success && success.length > 0) { %>
        <div class="alert alert-info alert-dismissible fade show" role="alert">
          <strong style="text-align: center;"><%= success[0] %></strong> 
          <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
      <% } %>
    
      <% if (error_upload_video && error_upload_video.length > 0) { %>
        <div class="alert alert-danger alert-dismissible fade show" role="alert">
          <strong style="text-align: center;"><%= error_upload_video %></strong> 
          <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
      <% } %>
      <% if(upload_video && upload_video.length>0){ %>
      <div class="alert alert-info alert-dismissible fade show" role="alert">
        <strong style="text-align: center;"><%= upload_video %></strong> 
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
      <% } %>


