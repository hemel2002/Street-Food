    <!-- MDB Bootstrap JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/3.10.2/mdb.min.js"></script>
    <script>
        document.addEventListener('DOMContentLoaded', () => {
            const videos = [
                "https://res.cloudinary.com/da7hqzvvf/video/upload/v1720197503/food/kqwj8plwxlvac4cspwms.mp4",
                "https://res.cloudinary.com/da7hqzvvf/video/upload/v1720197804/food/zodx0o3pw7fqzbkgreop.mp4",
                "https://res.cloudinary.com/da7hqzvvf/video/upload/v1720198903/food/wxca5rnlhwlfr5yba5xz.mp4",
                "https://res.cloudinary.com/da7hqzvvf/video/upload/v1720212477/food/zbyd87lydoayozcotofx.mp4",
                "https://res.cloudinary.com/da7hqzvvf/video/upload/v1720215242/food/vi5nee8ufxe0jcagerby.mp4",
                "https://res.cloudinary.com/da7hqzvvf/video/upload/v1720512025/food/ta7h0rrmx6gfarnn5sn8.mp4"
            ];

            let currentPage = 0;
            const itemsPerPage = 1;

            function openModal(page) {
                currentPage = page;
                updateVideoSource();
                updatePagination();
                const modal = new mdb.Modal(document.getElementById('videoModal'));
                modal.show();
            }

            function updateVideoSource() {
                const videoSource = document.getElementById('videoSource');
                videoSource.src = videos[currentPage];
                const videoPlayer = document.getElementById('videoPlayer');
                videoPlayer.load();
            }

            function updatePagination() {
                const totalPages = Math.ceil(videos.length / itemsPerPage);
                const paginationContainer = document.querySelector('.pagination');

                // Remove existing page links
                while (paginationContainer.children.length > 2) {
                    paginationContainer.removeChild(paginationContainer.children[1]);
                }

                // Add page links
                for (let i = 0; i < totalPages; i++) {
                    const li = document.createElement('li');
                    li.className = 'page-item' + (i === currentPage ? ' active' : '');
                    const a = document.createElement('a');
                    a.className = 'page-link';
                    a.href = '#';
                    a.textContent = i + 1;
                    a.onclick = (event) => {
                        event.preventDefault();
                        openModal(i);
                    };
                    li.appendChild(a);
                    paginationContainer.insertBefore(li, paginationContainer.children[paginationContainer.children.length - 1]);
                }
            }

            // Event listeners for previous and next buttons
            document.getElementById('prevPage').onclick = (event) => {
                event.preventDefault();
                if (currentPage > 0) {
                    openModal(currentPage - 1);
                }
            };

            document.getElementById('nextPage').onclick = (event) => {
                event.preventDefault();
                const totalPages = Math.ceil(videos.length / itemsPerPage);
                if (currentPage < totalPages - 1) {
                    openModal(currentPage + 1);
                }
            };

            // Event listener for "Play in Modal" buttons
            document.querySelectorAll('.play-video-btn').forEach(button => {
                button.addEventListener('click', (event) => {
                    const videoSrc = button.getAttribute('data-video-src');
                    document.getElementById('videoSource').src = videoSrc;
                    document.getElementById('videoPlayer').load();
                    const modal = new mdb.Modal(document.getElementById('videoModal'));
                    modal.show();
                });
            });
        });
    </script>
